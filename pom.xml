	<build>
		<finalName>agentProxy</finalName>
		<plugins>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<configuration>
					<webApp>
						<contextPath>/agentProxy</contextPath>
					</webApp>
					<connectors>
						<!-- <connector implementation="org.mortbay.jetty.nio.SelectChannelConnector"> -->
						<connector implementation="org.eclipse.jetty.server.nio.SelectChannelConnector">
							<port>8088</port>
							<maxIdleTime>60000</maxIdleTime>
						</connector>
					</connectors>
					<scanIntervalSeconds>10</scanIntervalSeconds>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<warName>${project.build.finalName}</warName>
					<packagingExcludes>
						WEB-INF/classes/META-INF/app.properties
					</packagingExcludes>
					<archive>
						<manifestEntries>
							<GIT-Revision-SHORT>${git.commit.id.abbrev}</GIT-Revision-SHORT>
							<GIT-Revision>${git.commit.id}</GIT-Revision>
							<COMMIT-Date>${git.commit.time}</COMMIT-Date>
							<BUILD-DATE>${git.build.time}</BUILD-DATE>
							<COMMIT-USER>${git.commit.user.name}</COMMIT-USER>
							<artifactId>${artifactId}</artifactId>
							<GIT-URL>${git.remote.origin.url}</GIT-URL>
							<GIT-BRANCH>${git.branch}</GIT-BRANCH>
							<COMMIT-MESSAGE>${git.commit.message.full}</COMMIT-MESSAGE>
							<COMMIT-ID-DESCRIBE>${git.commit.id.describe}</COMMIT-ID-DESCRIBE>
							<version>${project.version}</version>
							<project>${project.description}</project>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>

			<plugin>
				<groupId>pl.project13.maven</groupId>
				<artifactId>git-commit-id-plugin</artifactId>
				<version>2.1.5</version>
				<executions>
					<execution>
						<goals>
							<goal>revision</goal>
						</goals>
						<!-- <phase>process-sources</phase> -->
					</execution>
				</executions>
				<configuration>
					<!--���ڸ�ʽ;Ĭ��ֵ:dd.MM.yyyy '@' HH:mm:ss z;-->
					<dateFormat>yyyy-MM-dd HH:mm:ss</dateFormat>
					<!--,����������,�Ƿ��ӡ��ϸ��Ϣ;Ĭ��ֵ:false;-->
					<verbose>true</verbose>
					<!-- ".git"�ļ�·��;Ĭ��ֵ:${project.basedir}/.git; -->
					<dotGitDirectory>${project.basedir}/.git</dotGitDirectory>
					<!--����Ŀ�������Ϊpom,�Ƿ�ȡ������;Ĭ��ֵ:true;-->
					<skipPoms>false</skipPoms>
					<!--�Ƿ�����"git.properties"�ļ�;Ĭ��ֵ:false;-->
					<generateGitPropertiesFile>false</generateGitPropertiesFile>
					<!--ָ��"git.properties"�ļ��Ĵ��·��(�����${project.basedir}��һ��·��);-->
					<!--<generateGitPropertiesFilename>target/classes/git.properties</generateGitPropertiesFilename>-->
					<!--".git"�ļ���δ�ҵ�ʱ,�����Ƿ�ʧ��;������true,�򹹽�ʧ��;������false,������ִ�и�Ŀ��;Ĭ��ֵ:true;-->
					<failOnNoGitDirectory>true</failOnNoGitDirectory>

					<!--git��������,��ѡ;��JGit�ṩʵ��;-->
					<gitDescribe>
						<!--�Ƿ�������������-->
						<skip>false</skip>
						<!--�ύ����δ����tagʱ,����ӡ�ύ����ID,-->
						<always>false</always>
						<!--�ύ����ID��ʽ�ַ�����,���ֵΪ:40;Ĭ��ֵ:7;
                          0������������;�����н���;
                        -->
						<abbrev>7</abbrev>
						<!--��������ʱ,�������޸�ʱ(��"dirty state"),����ָ����׺;Ĭ��ֵ:"";-->
						<dirty>-dirty</dirty>
						<!--always print using the "tag-commits_from_tag-g_commit_id-maybe_dirty" format, even if "on" a tag.
                          The distance will always be 0 if you're "on" the tag.
                        -->
						<forceLongFormat>false</forceLongFormat>
					</gitDescribe>
				</configuration>
			</plugin>
		</plugins>

